{
  "extends": [
    "next/core-web-vitals",
    "eslint-config-prettier",
    "airbnb-typescript"
  ],
  "plugins": ["eslint-plugin-prettier"],
  "parserOptions": {
    "project": ["./tsconfig.json"],

    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2023,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "@typescript-eslint/no-unused-vars": "warn"
  }
}
